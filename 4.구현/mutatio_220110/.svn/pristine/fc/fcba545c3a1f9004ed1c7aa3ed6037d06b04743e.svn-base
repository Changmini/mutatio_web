					<div class="modal-footer">
						<button id="btn_reply_modify" type="button"
							class="btn btn-sea">댓글 수정</button>
						<button type="button" id="btn_modal_close" class="btn btn-sea"
							data-dismiss="modal">닫기</button>
					</div>
				</form>
