table.table-design {
	width:90%;
	margin-left : 0px;
	border-collapse: collapse;
	text-align: left;
	line-height: 1.5;
	border-bottom: 1px solid #ccc;
}
table.table-design > thead th {
	padding: 10px;
	padding-top: 13px;
	padding-bottom: 20px;
	font-weight: bold;
	vertical-align: top;
	color: #ffffff;
	background: #2FAF53;
	margin: 20px 10px;
	text-align:center;
}
table.table-design tbody th {
	width: 5%;
	padding: 10px;
}
table.table-design td {
	width: 20%;
	padding: 10px;
	vertical-align: top;
}

.innerTb tr:nth-child(odd){
    background: #efefef;
}

.fas{
	cursor:pointer;
	margin-top : 5px;
}

.conbox.con1{
	height : 700px;
	margin-bottom : 100px;
	overflow-x:hidden;
	overflow-y:scroll;
}

.conbox.con1 table > tbody td{
	border-bottom: 1px solid #d3d7cf;
    padding: 7px;
    vertical-align: middle;

}

.conbox.con1 table > tbody td:nth-child(n+4):nth-child(-n+6){
	text-align : center;
}

.conbox.con2{
	width : 100%;
	height : 1300px;
}

