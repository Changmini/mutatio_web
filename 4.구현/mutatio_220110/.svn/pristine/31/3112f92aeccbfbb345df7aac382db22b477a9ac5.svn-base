input[type="radio"] {display: none;}
input[type="radio"] + label{
	display: inline-block;
	padding-top:10px;
	padding-left:40px;
	padding-right:30px;
	padding-bottom:25px;
	color: #555753;
	font-size : 15px;
	font-weight : 500;
	border-radius : 10px;
	border : 2px solid #2FAF53;
	cursor: pointer;
}
input[type="radio"] + label:hover{
	color: #2FAF53;
}

input[type="radio"]:checked + label {
    background: #2FAF53;
    color: white;
}

input[type="radio"]:checked + label > .fas {
    color: white;
}


.his_indiv_menu > input[type="radio"] + label{
border: none;
}

.his_indiv_menu > input[type="radio"] + label{
	/* padding : 14px;
	background:white;
	font-size : 15px;
	font-weight : 500;
	color : black;
	border-right : 2px solid lightgray;
	border-radius : 0px;
	cursor : pointer; */
	display: inline-block;
	padding-top:10px;
	padding-left:30px;
	padding-right:30px;
	padding-bottom:14px;
	color: #555753;
	font-size : 15px;
	font-weight : 500;
	border-radius : 10px;
	border : 2px solid #2FAF53;
	cursor: pointer;
}

.his_indiv_menu > input[type="radio"]:checked + label{
	background: #2FAF53;
    color: white;
}

.his_indiv_menu > label:hover{
	color: #2FAF53;
}

.his_indiv_menu > label:first-child{
	border-left : 2px solid lightgray;
}





