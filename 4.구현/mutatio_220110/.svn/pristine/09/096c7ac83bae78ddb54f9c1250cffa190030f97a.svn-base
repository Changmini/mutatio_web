#wrap{
	text-align: center;
}
.container {
	width: auto;
	height: 1020px; 
	display: inline-flex;
	position: relative;
	top: -50px;
	margin: 0px;
}
.detailsList {
    width: 370px;
    height: 750px;
    float: left;
    text-align: right;
}
.contents{
    width: 100%;
    height: 100%;
    float: left;
    text-align: left;
}



/* check radio design */
.detailsList > div {
    width: 300px;
    height: 750px;
    display: inline-block;
    text-align: center;
    padding-right: 30px;
}
.detailsList > div > div > h4{
    margin: 10px;
    padding: 5px;
    background-color: rgb(245, 245, 245);
}
.detailsList > div > div:not(:first-child) {
    width: 250px;
    height: auto;
    padding: 10px;
}
.detailsList > div > div:not(:last-child){
    border-bottom: solid 1px lightgray;
}
.c_h2 {
    width: 100%;
    height: 80px;
    text-align: center;
    border-bottom: solid 2px black;
    background-color: rgb(248, 248, 248);
}
.c_h2 > h2 {
    padding-top: 20px;
}





/* TABLE DESIGN */
#div_table {
    width: 1080px;
    height: 100%;
    margin: 0px;
}

table.type08 {
	width:1080px;
	height: 100%;
	border-collapse: collapse;
	text-align: left;
	line-height: 1.5;
	border-left: 1px solid #ccc;
}

table.type08 thead th {
  padding: 10px;
  font-weight: bold;
  border-top: 1px solid #ccc;
  border-right: 1px solid #ccc;
  border-bottom: 2px solid #c00;
  background: #dcdcd1;
}
table.type08 td {
  padding: 10px;
  vertical-align: top;
  border-right: 1px solid #ccc;
  border-bottom: 1px solid #ccc;
}




.div_paging {
    width: 100%;
    height: 50px;
    text-align: center;
    margin-top: 20px;
}
.div_paging_a{
    width: 300px;
    height: 30px;
    text-align: center;
    padding-top: 23px;
}














