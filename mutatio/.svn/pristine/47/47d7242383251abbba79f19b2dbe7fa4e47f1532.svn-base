.div_footer {
    width: 100%;
    height: 145px;
    background-color: rgb(0, 0, 0);
    display: flex;
}
.div_footer > div {
    padding-top: 30px;
    color: #fff;
}
.div_footer > div:first-child {
    flex: 4;
}
.div_footer > div:last-child {
    flex: 6;
}
.div_footer > div:first-child{
    text-align: center;
    font-weight: bold;
}