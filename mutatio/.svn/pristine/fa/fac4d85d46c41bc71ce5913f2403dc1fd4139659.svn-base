

.contents{
    width: 100%;
    height: 700px;
    /* background-color: lightgreen; */
}
.content01 {
    width: 1280px;
    height: 700px;
    /* background-color: lightgrey; */
}
.content01_h1 {
    width: 100%;
    height: 100px;
    text-align: center;
    border-bottom: 1px solid black;
    margin-bottom: 20px; margin-top: 20px;
    background-color: rgb(248, 248, 248);
}
.content01_h1 > h2 {
    padding-top: 30px;
}
/* 테이블 영역잡기 */
#div_table {
    width: 1280px;
    height: 500px;
    /* background-color: lightsalmon; */
}
#div_table td {
    padding: 10px;
}
#div_table , .td_left{
    text-align: center;
}
.td_right{
    text-align: left;
}
.td_right > input {
    width: 100%;
    height: 25px;
    border-radius: 5px;
    border:none;
    background: rgba(0, 0, 0, 0.1);
}
.td_right > textarea {
    width: 100%;
    height: 200px;
    border-radius: 5px;
    border:none;
    background-color: rgba(0, 0, 0, 0.1);
}
#div_button {
    width: 1280px;
    height: 100px;
    text-align: right;
    /* background-color: lightskyblue; */
}
#div_button input {
    padding: 5px;
    border:none;
    border-radius: 5px;
}
