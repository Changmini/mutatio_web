.contents{
    width: 100%;
    height: 100%px;
    text-align: center;
}
.contents > div{
	margin-top:30px;
}
#mychart{
	border-style: solid;
	border-width: 2px;
	border-color: LightGray; 
	padding: 12px;
	word-break: break-all;
}
#myChart {
	margin-top:30px;
	width: 950px;
	height: 500px;
	overflow: scroll;
}

.txtBox0, .txtBox1, .txtBox2 {
	border-style: solid;
	border-width: 2px 2px 2px 8px;
	padding: 12px;
	word-break: break-all;
	border-color: DodgerBlue;
	background-color:rgba(30, 144, 255, 0.2);
}
.news_div{
	width: 950px;
}
.news0, .news1{
	margin-top:30px;
	border-style: solid;
	border-width: 2px;
	border-color: LightGray; 
	padding: 12px;
	word-break: break-all;
	background-color: rgb(241, 241, 241);
}

.top{
	margin-bottom:15px;
}
.container{
	display: flex;
}
.container > .content, .news1 > .content{
	padding: 30px;
	float: left;
	display: block;
	text-align: left;
}
.container > .img, .news1 > .img{
	float: left;
	display: block;
}
.factor_div{
	width: 950px;
	height: 500px;
	overflow-x:hidden;
	overflow-y:scroll;
}
.table-design{
	margin-top:30px;
	width: 950px;
	border-style: solid;
	border-width: 2px;
	border-color: LightGray; 
	border-collapse: collapse;
	table-layout:fixed;
	text-align: center;
	line-height: 1.5;
	
}
.table-design thead th {
	padding: 10px;
	font-weight: bold;
	vertical-align: top;
	color: #369;
	border-bottom: 3px solid #036;
 }
 .table-design thead th:nth-child(3) {
	text-align: left;
	padding: 10px 10px 10px 18px;
 }
 .table-design tbody th {
 	border-bottom: 1px solid #ccc;
 }
.table-design td {
	width: 350px;
	padding: 10px;
	vertical-align: top;
	border-bottom: 1px solid #ccc;
	text-overflow: ellipsis;
	overflow: hidden;
	white-space: nowrap;
}
.table-design td:nth-child(2), td:nth-child(3){
	text-align: left;
}
.table-design tr:nth-child(2n) {
	background: rgb(241, 241, 241);
  }
 #page_footer{
 	margin-top: 70px;
 }
  